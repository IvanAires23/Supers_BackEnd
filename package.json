{
  "name": "supers",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "build": "tsc",
    "start": "node ./dist/server.js",
    "start:prod": "npm run dev:migration:run && npm run start",
    "test": "npm run test:load-envs -- jest --detectOpenHandles -- --runInBand",
    "test:cov": "npm run test:load-envs -- jest -- --coverage --runInBand",
    "test:load-envs": "dotenv -e .env.test",
    "test:migration:run": "npm run test:load-envs prisma migrate deploy",
    "test:migration:generate": "npm run test:load-envs prisma migrate dev",
    "dev": "cross-env NODE_ENV=development nodemon ./src/server.ts",
    "dev:migration:run": "npm run dev:load-envs prisma migrate deploy",
    "dev:migration:generate": "npm run dev:load-envs prisma migrate dev",
    "dev:load-envs": "dotenv -e .env.development"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.1.1",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^7.3.0",
    "dotenv-expand": "^10.0.0",
    "express": "^4.18.2",
    "http-status": "^1.7.3",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "mysql": "^2.18.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.2.0",
    "@types/bcrypt": "^5.0.1",
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.20",
    "@types/jest": "^29.5.6",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/supertest": "^2.0.15",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prisma": "^5.1.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}